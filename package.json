{
	"name": "express-node-server-babel",
	"version": "0.1.0",
	"description": "Express server using ES2015",
	"main": "index.js",
	"scripts": {
		"start": "nodemon src --exec babel-node",
		"build": "babel src -d dist",
		"serve": "node dist/index.js",
		"test": "mocha --compilers js:babel-register",
		"eslint": "eslint",
		"precommit": "pretty-quick --staged"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-preset-es2015": "6.24.1",
		"babel-preset-stage-2": "6.24.1",
		"chai": "4.1.2",
		"chai-http": "4.2.0",
		"eslint": "5.5.0",
		"husky": "0.14.3",
		"mocha": "5.2.0",
		"nodemon": "1.18.4",
		"prettier": "1.14.2",
		"pretty-quick": "1.6.0"
	},
	"dependencies": {
		"express": "4.16.3"
	}
}
